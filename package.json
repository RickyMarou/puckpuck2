{
  "name": "template-vite-ts",
  "description": "A Phaser 3 TypeScript template using Vite.",
  "version": "1.3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-vite-ts.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-vite-ts/issues"
  },
  "homepage": "https://github.com/phaserjs/template-vite-ts#readme",
  "scripts": {
    "dev": "vite --config vite/config.dev.mjs",
    "build": "vite build --config vite/config.prod.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "types": "tsc --noEmit",
    "format": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs",
    "check": "pnpm run types && pnpm run format && pnpm run lint && pnpm run test:run"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "terser": "^5.42.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "phaser": "^3.90.0"
  }
}
